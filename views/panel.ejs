<body>
<body>


    <p>Użytkownik: {{users[0].username}}</p>
    Akcje
    <form method="POST" action="/user/{{users[0].id}}/delate">
        <input type="hidden" name="id" value="{{users[0].id}}">
        <button type="submit">Usuń</button>
      </form>

      <form method="POST" action="/user/{{users[0].id}}/changeName">
        <input type="hidden" name="id" value="{{users[0].id}}">
        <input type="text" name="name" placeholder="{{users[0].username}}">
        <button type="submit">Zmień nazwę</button>
      </form>
</body>
